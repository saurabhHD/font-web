RewriteEngine On
RewriteRule ^script$  404.html
RewriteRule ^privacy$ privacy.php
RewriteRule ^about-us$ about.php
RewriteRule ^terms$ terms.php
#RewriteCond %{SERVER_PORT} 80
#RewriteCond %{HTTP_HOST} ^(www\.)?yourdomain\.com
#RewriteRule ^(.*)$ https://yourdomain.com [R,L]
ErrorDocument 404 https://yourdomain.com/404.html
ErrorDocument 403 https://yourdomain.com/404.html